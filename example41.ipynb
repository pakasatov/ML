    "#example 4-1\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "data = [[3, 10],   #คอลัมน์ x, y\n",
    "        [5, 7],\n",
    "        [6, 6],\n",
    "        [7, 4.5],\n",
    "        [9, 3.25]]\n",
    "\n",
    "arr = np.array(data)\n",
    "x = arr[:, 0]\n",
    "y = arr[:, 1]\n",
    "\n",
    "plt.figure(figsize=(4, 3))\n",
    "\n",
    "plt.scatter(x, y, s=50)\n",
    "plt.show()"
   ]
